{
  "manifest_version": 3,
  "name": "Auto Close New Tabs",
  "version": "1.1",
  "description": "Automatically closes newly opened tabs unless disabled.",
  "permissions": ["tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Toggle Tab Closer",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
